{
  "compilerOptions": {
      "incremental": false,
      "target": "ES2015",
      "module": "commonjs",
      "outDir": "lib",
      "strict": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
  }
}